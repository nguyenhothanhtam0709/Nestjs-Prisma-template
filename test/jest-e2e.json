{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@bootstrap": "../src/bootstrap",
    "@commons/(.*)": "<rootDir>/../src/commons/$1",
    "@modules/(.*)": "<rootDir>/../src/modules/$1"
  }
}
